{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/timeline.css",
      "dest": "/timeline.css"
    },
    {
      "src": "/mobile.css",
      "dest": "/mobile.css"
    },
    {
      "src": "/data/(.*)",
      "dest": "/data/$1"
    },
    {
      "src": "/timeline-optimized.js",
      "dest": "/timeline-optimized.js"
    },
    {
      "src": "/(.*)",
      "dest": "/timeline.html"
    }
  ],
  "env": {
    "PORT": "3000",
    "NODE_ENV": "production"
  }
}
